wave modify -driver freeze -pattern clock -initialvalue 0 -period 500ps -dutycycle 50 -starttime 0ps -endtime 1000ps Edit:/xbar_v1/s
add wave -position insertpoint  \
sim:/xbar_v1/x1
wave modify -driver freeze -pattern clock -initialvalue 0 -period 500ps -dutycycle 50 -starttime 0ps -endtime 500ps Edit:/xbar_v1/s
